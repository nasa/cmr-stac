{
  "name": "cmr-stac-api-proxy",
  "version": "1.0.0",
  "description": "An implementation of the STAC API on top of NASA's [Common Metadata Repository](https://cmr.earthdata.nasa.gov/search/).",
  "scripts": {
    "postinstall": "node bin/install.js; bin/get_stac_spec_schemas.sh",
    "test": "jest --detectOpenHandles --forceExit --env=node --coverage --reporters=jest-junit --reporters=default",
    "start": "cd search; npm start",
    "lint": "cd search; npm run lint"
  },
  "repository": "https://github.com/Element84/cmr-stac-api-proxy",
  "author": "Element 84",
  "license": "Apache-2.0",
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "axios": "0.18.1",
    "eslint": "^6.1.0",
    "eslint-config-semistandard": "^14.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^24.9.0",
    "jest-junit": "^12.0.0",
    "serverless": "^1.49.0",
    "serverless-offline": "^5.8.0"
  }
}
