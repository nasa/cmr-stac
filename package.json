{
  "name": "CMR-STAC",
  "version": "2.0.0",
  "description": "NASA' Common Metadata Repository STAC implementation.",
  "repository": "https://github.com/NASA/cmr-stac",
  "license": "NASA-1.3",
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true IS_LOCAL=true mocha",
    "test:coverage": "cross-env TS_NODE_FILES=true IS_LOCAL=true nyc --reporter=html --reporter=text mocha",
    "test:watch": "cross-env TS_NODE_FILES=true IS_LOCAL=true mocha --watch",
    "dev": "cross-env SLS_DEBUG=* IS_LOCAL=true serverless offline start --reloadHandler --config serverless.dev.yml --stage dev",
    "deploy": "sls deploy",
    "remove": "sls remove",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@serverless/event-mocks": "^1.1.1",
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/chai": "^4.3.4",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/serverless": "^3.12.8",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "ajv": "^8.11.0",
    "ajv-formats-draft2019": "^1.6.1",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "json-schema-to-typescript": "^11.0.2",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "mocha-suppress-logs": "^0.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "serverless": "^3.24.1",
    "serverless-offline": "^11.4.0",
    "serverless-plugin-log-subscription": "^2.1.5",
    "serverless-plugin-typescript": "^2.1.4",
    "serverless-s3-sync": "^3.1.0",
    "sinon": "^14.0.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "helmet": "^6.1.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "source-map-support": "^0.5.21"
  }
}
